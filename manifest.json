{
  "manifest_version": 3,
  "name": "Open WebCull",
  "version": "1.1",
  "description": "A shortcut to open (or retrieve the tab of) your WebCull bookmark manager.",
  "action": {
    "default_icon": "icons/webcull-icon-16.png",
    "default_title": "Open WebCull"
  },
  "icons": {
    "16": "icons/webcull-icon-16.png",
    "32": "icons/webcull-icon-32.png",
    "48": "icons/webcull-icon-48.png",
    "128": "icons/webcull-icon-128.png"
  },
  "host_permissions": [
    "https://app.webcull.com/*"
  ],
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}